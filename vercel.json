{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "python3.12",
            "maxDuration": 60,
            "memory": 3008
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}